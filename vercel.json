{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go",
      "config": {
        "includeFiles": [
          "web/templates/**",
          "web/static/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.go"
    }
  ],
  "env": {
    "CGO_ENABLED": "0",
    "GO111MODULE": "on",
    "GO_BUILD_FLAGS": "-ldflags '-s -w'"
  }
}
